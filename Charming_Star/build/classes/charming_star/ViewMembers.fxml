<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="503.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="charming_star.ViewMembersController">
    <Label layoutX="264.0" layoutY="27.0" style="-fx-font-size: 20px;">
      <font>
         <Font name="Tahoma Bold" size="14.0" />
      </font></Label>
    
    <TableView fx:id="membersTable" layoutX="28.0" layoutY="88.0" prefHeight="335.0" prefWidth="604.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="50" text="ID" />
            <TableColumn fx:id="nameColumn" prefWidth="150" text="Name" />
            <TableColumn fx:id="ageColumn" prefWidth="100" text="Age" />
            <TableColumn fx:id="membershipTypeColumn" prefWidth="150" text="Membership Type" />
            <TableColumn fx:id="joinDateColumn" prefWidth="150" text="Join Date" />
        </columns>
    </TableView>

    <Button layoutX="200.0" layoutY="441.0" onAction="#handleUpdate" text="Update" />
    <Button layoutX="307.0" layoutY="441.0" onAction="#handleDelete" text="Delete" />
    <Button layoutX="411.0" layoutY="441.0" onAction="#goBack" text="Back" />
   <Label layoutX="265.0" layoutY="30.0" text="Gym Members" textFill="#1a027b">
      <font>
         <Font name="Tahoma Bold" size="18.0" />
      </font>
   </Label>
</AnchorPane>
