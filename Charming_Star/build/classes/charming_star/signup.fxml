<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="325.0" prefWidth="349.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="charming_star.SignupController">
    <children>
        <TextField fx:id="usernameField" layoutX="100" layoutY="50" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="100" layoutY="100" promptText="Password" />
        <PasswordField fx:id="confirmPasswordField" layoutX="100" layoutY="150" promptText="Confirm Password" />
        <Button fx:id="signupButton" layoutX="100" layoutY="200" onAction="#handleSignup" text="Sign Up" />
        <Button layoutX="100" layoutY="250" onAction="#goToLogin" text="Go to Login" />
    </children>
</AnchorPane>
