<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="301.0" prefWidth="318.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="charming_star.LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="100" layoutY="50" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="100" layoutY="100" promptText="Password" />
        <Button fx:id="loginButton" layoutX="100" layoutY="150" onAction="#handleLogin" text="Login" />
        <Button layoutX="100" layoutY="200" onAction="#goToSignup" text="Go to Signup" />
    </children>
</AnchorPane>
